{
   "name": "node",
   "version": "1.0.0",
   "description": "NodeJS",
   "main": "dist/index.js",
   "scripts": {
      "clean": "rm -fr dist;rm -fr deploy",
      "generate-sea": "./sea-config.sh",
      "watch": "node dist/esbuild/watch.js",
      "start": "node dist/index.js",
      "compile": "tsc",
      "build": "npm run clean;npm run compile;cp schema.graphql dist/schema.graphql;cp -r -t dist src/static;rm -fr dist/static/styles;rm -fr dist/static/lib/bundle;node dist/esbuild/prod.js",
      "buildStart": "npm run build && npm start",
      "test": "jest",
      "codegen": "graphql-codegen-esm --config codegen.ts"
   },
