{
   "name": "node",
   "version": "1.0.0",
   "type": "module",
   "description": "NodeJS",
   "main": "src/index.ts",
   "scripts": {
      "dts": "node --loader ts-node/esm src/dts.js",
      "start": "node --no-warnings=ExperimentalWarning --loader ts-node/esm src/index.ts",
      "test": "jest",
      "codegen": "graphql-codegen-esm --config codegen.ts",
      "webpack:dev": "node --loader ts-node/esm node_modules/webpack-dev-server/bin/webpack-dev-server.js --env NODE_ENV=development",
      "webpack:prod": "node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js --env NODE_ENV=production --config webpack.config.ts"
   },
   "author": "Serkan YESILDAG",
   "license": "MIT",
   "devDependencies": {
      "@graphql-codegen/cli": "5.0.2",
      "@graphql-codegen/introspection": "4.0.3",
      "@graphql-codegen/typescript": "4.0.9",
      "@jest/globals": "^28.1.3",
      "@swc/core": "^1.7.22",
      "@types/compression": "^1.7.5",
      "@types/cookie-parser": "^1.4.7",
      "@types/express": "^4.17.21",
      "@types/node": "^20.16.2",
      "@types/react": "^18.3.5",
      "@types/react-dom": "^18.3.0",
      "@types/serve-favicon": "^2.5.7",
      "@types/webpack": "^5.28.5",
      "jest": "^28.1.3",
      "react-refresh": "^0.14.2",
      "ts-jest": "^28.0.8",
      "ts-jest-resolver": "^2.0.0",
      "ts-loader": "^9.5.1",
      "ts-node": "^10.9.2",
      "typescript": "^4.9.5",
      "webpack": "^5.94.0",
      "webpack-cli": "^5.1.4",
      "webpack-dev-server": "^5.0.4"
   },
   "dependencies": {
      "@tensorflow-models/mobilenet": "^2.1.1",
      "@tensorflow/tfjs-node": "^4.20.0",
      "compression": "^1.7.4",
      "cookie-parser": "^1.4.6",
      "dotenv": "^16.4.5",
      "express": "^4.19.2",
      "graphiql": "^3.7.1",
      "graphql": "^16.9.0",
      "graphql-http": "^1.22.1",
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "serve-favicon": "^2.5.0"
   }
}
