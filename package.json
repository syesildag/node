      "@tensorflow/tfjs-node": "^4.20.0",
      "@uiw/react-json-view": "^2.0.0-alpha.27",
      "compression": "^1.7.4",
      "cookie-parser": "^1.4.6",
      "dotenv": "^16.4.5",
