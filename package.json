      "@jest/globals": "^28.1.3",
      "@swc/core": "^1.7.22",
      "@types/compression": "^1.7.5",
      "@types/cookie-parser": "^1.4.7",
      "@types/express": "^4.17.21",
      "@types/jsdom": "^21.1.7",
      "@types/node": "^20.16.2",
      "@types/node-schedule": "^2.1.7",
      "@types/react": "^18.3.5",
      "@types/react-dom": "^18.3.0",
      "@types/serve-favicon": "^2.5.7",
      "esbuild": "0.23.1",
      "jest": "^28.1.3",
      "postject": "^1.0.0-alpha.6",
      "ts-jest": "^28.0.8",
